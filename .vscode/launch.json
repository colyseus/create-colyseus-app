{
    "version": "0.2.0",
    "configurations": [
      {
        "name": "Debug",
        "type": "pwa-node",
        "request": "launch",
        "args": ["src/index.js"],
        "cwd": "${workspaceRoot}",
        "internalConsoleOptions": "openOnSessionStart",
        "resolveSourceMapLocations": [
          "${workspaceFolder}/**",
          "!**/node_modules/**"
        ],
        "skipFiles": ["<node_internals>/**", "node_modules/**"]
      }
    ]
  }
